                </Grid>
            </WrapPanel>
        </Grid>
        <Grid Height="360" HorizontalAlignment="Left" Margin="660,0,0,0" Name="gameMessageGrid" VerticalAlignment="Top" Width="200">
            <GroupBox Header="Game Information" Height="360" HorizontalAlignment="Left" Name="gameMessageGroupBox" VerticalAlignment="Top" Width="200" Foreground="#FF5CFFF1">
                <Grid>
                    <Border BorderBrush="Silver" BorderThickness="1" Height="30" HorizontalAlignment="Left" Margin="0,30,0,0" Name="player1Border" VerticalAlignment="Top" Width="72">
                        <TextBlock Name="player1TextBlock" Text="Player1" TextAlignment="Center" FontSize="16" FontWeight="Bold" Foreground="White" />
                    </Border>
                    <Border BorderBrush="Silver" BorderThickness="1" Height="30" HorizontalAlignment="Left" Margin="115,30,0,0" Name="player2Border" VerticalAlignment="Top" Width="72">
                        <TextBlock Name="player2TextBlock" Text="Player2" FontWeight="Bold" TextAlignment="Center" FontSize="16" Foreground="White" />
                    </Border>
                    <Border BorderBrush="{x:Null}" BorderThickness="1" Height="36" HorizontalAlignment="Left" Margin="73,27,0,0" Name="vsBorder" VerticalAlignment="Top" Width="40">
                        <TextBlock Name="VSTextBlock" Text="VS" TextAlignment="Center" FontSize="24" FontWeight="Bold" Foreground="#FF93D9FF" />
                    </Border>
                    <TextBlock Height="25" HorizontalAlignment="Left" Margin="0,71,0,0" Name="player1StateTextBlock" Text="Wait for connection" VerticalAlignment="Top" Width="105" TextAlignment="Center" FontWeight="Normal" FontSize="10" Foreground="LightPink" />
                    <TextBlock Height="25" HorizontalAlignment="Left" Margin="105,71,0,0" Name="player2StateTextBlock" Text="Wait for connection" VerticalAlignment="Top" Width="83" TextAlignment="Center" FontSize="10" FontWeight="Normal" Foreground="LightPink" />
                    <TextBlock Height="36" HorizontalAlignment="Left" Margin="6,291,0,0" Name="turnNameTextBlock" Text="Turn:" VerticalAlignment="Top" Width="66" FontSize="26" TextAlignment="Center" Foreground="#FF9CF889" Visibility="Hidden" />
                    <TextBlock Height="27" HorizontalAlignment="Left" Margin="78,297,0,0" Name="turnTextBlock" VerticalAlignment="Top" Width="109" FontSize="18" TextAlignment="Center" Foreground="White" Text="{Binding ElementName=player1TextBlock, Path=Text}" Visibility="Hidden" />
                    <Button Content="Ready" Height="45" HorizontalAlignment="Left" Margin="33,146,0,0" Name="readyButton" VerticalAlignment="Top" Width="120" Click="readyButton_Click" />
                    <Button Content="Cancel" Height="45" HorizontalAlignment="Left" Margin="33,211,0,0" Name="unReadyButton" VerticalAlignment="Top" Width="120" IsEnabled="False" Click="unReadybutton_Click" />
                </Grid>
            </GroupBox>
        </Grid>
        <Grid Height="267" HorizontalAlignment="Left" Margin="660,368,0,0" Name="showTextGrid" VerticalAlignment="Top" Width="200">
            <TextBox Height="267" HorizontalAlignment="Left" Name="showTextBox" VerticalAlignment="Top" Width="200" />
        </Grid>
        <Grid Height="81" HorizontalAlignment="Left" Margin="660,642,5,5" Name="sendTextGrid" VerticalAlignment="Top" Width="202" Background="{x:Null}">
            <TextBox Height="77" HorizontalAlignment="Left" Name="sendTextBox" VerticalAlignment="Top" Width="138" />
            <Button Content="Send" Height="53" HorizontalAlignment="Right" Name="sendMessageButton" VerticalAlignment="Top" Width="58" Margin="0,24,3,0" Click="sendMessageButton_Click" />
        </Grid>
        <Grid.Background>
            <RadialGradientBrush>
                <GradientStop Color="Black" Offset="0" />
                <GradientStop Color="#FF2B4D5F" Offset="1" />
            </RadialGradientBrush>
        </Grid.Background>
    </Grid>
</Window>
